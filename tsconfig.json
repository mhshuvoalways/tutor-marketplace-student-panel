{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/components/common/header/index.jsx",
    "app/layout.js",
    "app/tutors/page.jsx",
    "app/components/qualifiedTutors/index.jsx"
, "app/page.jsx", "app/components/common/upload/index.jsx", "app/components/common/dialog/index.jsx"  ],
  "exclude": ["node_modules"]
}
